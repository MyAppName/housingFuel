<view class="content"><view class="nav"><text class="fontcolor">选择其他地址</text></view><view class="line"></view><block wx:for="{{getAddress}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="ul"><view data-event-opts="{{[['tap',[['click',[index]]]]]}}" class="li" bindtap="__e"><view class="li_info"><view class="li_l"><view class="li_text"><view class="li_l1"><text class="fontstyle1">{{item.name}}</text><text class="fontstyle1">{{''+item.phone}}</text></view><view class="li_l2"><text class="fontstyle1">{{item.region+"  "+item.street+"  "+item.address}}</text></view></view></view><view class="li_r"><view data-event-opts="{{[['tap',[['deleteItem',[index]]]]]}}" class="li_img" bindtap="__e"><image src="cloud://housingfuel-jcgsv.686f-housingfuel-jcgsv-1300436900/static/member/delete.png" mode></image></view></view></view></view></view></block><view class="buttons"><button class="button1" type data-event-opts="{{[['tap',[['addNewAddress',['$event']]]]]}}" bindtap="__e">添加新地址</button><button class="button2" type data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e">保存</button></view></view>